scanf("%d%d%d",&x,&y,&z);
    int max2;
    if(x>y) max2=x;
        else max2=y;
    if (max2>z) printf("max to %d",max2);
        else printf("max to %d",z);

w domu, zrob ten co przeliczyl srodkowa z min i max (a+b+c-min-max=srodek)

    int a,h,b,c,kom;
    scanf("%d",&kom);
    //1=podaj a i h
    //2=podaj a,b,c
    if(kom==1)
    {
        printf("podaj a, oraz h\n");
        scanf("%d%d",&a,&h);
        float pole=0.5*a*h;
        printf("%f",pole);
    }
 if(kom==2)
    {
        printf("podaj a,b,oraz c\n");
        scanf("%d%d%d",&a,&b,&c);
        float p=(a+b+c)/2;
        float pole=sqrt(p*(p-a)*(p-b)*(p-c));
        printf("%f",pole);
    }


    return 0;
