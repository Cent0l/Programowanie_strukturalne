      int a,b,c;
   float delta,x1,x2;
   scanf("%d%d%d",&a,&b,&c);
   printf("twoje rownanie to %dx^2+%dx+%d=0\n",a,b,c);
   delta=(b*b)-(4*a*c);
   if(delta<0)
   {
   printf("brak rozwiazan rzeczywistych");
   }
   else if(delta==0)
   {
    x1=(-b)/(2*a);
    printf("%f",x1);
   }
   else if(delta>0)
   {
   x1=(-b-sqrt(delta))/(2*a);
   x2=(-b+sqrt(delta))/(2*a);
   if(x1==x2)
    printf("%f",x1);
    else printf("%f\n%f",x1,x2);
   }

